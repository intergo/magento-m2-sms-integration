<csp_whitelist xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
               xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Csp:etc/csp_whitelist.xsd">
    <policies>
        <policy id="script-src">
            <values>
                <value id="www_smsto_js_base" type="host"></value>
                <value id="www_smsto_js_assets" type="host">https://integration.sms.to/</value>
            </values>
        </policy>
        <policy id="style-src">
            <values>
                <value id="www_smsto_css_base" type="host"></value>
                <value id="www_smsto_css_assets" type="host">https://integration.sms.to/</value>
            </values>
        </policy>
    </policies>
</csp_whitelist>
