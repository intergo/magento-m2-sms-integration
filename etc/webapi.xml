<?xml version="1.0" ?>
<routes xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Webapi:etc/webapi.xsd">
    <route method="GET" url="/V1/smsto-sms/params">
		<service class="Smsto\Sms\Api\SmslogRepositoryInterface" method="getParams"/>
		<resources>
			<resource ref="anonymous" />
		</resources>
	</route>
    <route method="GET" url="/V1/smsto-sms/call">
		<service class="Smsto\Sms\Api\SmslogRepositoryInterface" method="callSmsto"/>
		<resources>
			<resource ref="anonymous" />
		</resources>
	</route>
	<route method="POST" url="/V1/smsto-sms/smslog">
		<service class="Smsto\Sms\Api\SmslogRepositoryInterface" method="save"/>
		<resources>
			<resource ref="Smsto_Sms::Smslog_save"/>
		</resources>
	</route>
	<route method="GET" url="/V1/smsto-sms/smslog/search">
		<service class="Smsto\Sms\Api\SmslogRepositoryInterface" method="getList"/>
		<resources>
			<resource ref="Smsto_Sms::Smslog_view"/>
		</resources>
	</route>
	<route method="GET" url="/V1/smsto-sms/smslog/:logId">
		<service class="Smsto\Sms\Api\SmslogRepositoryInterface" method="getById"/>
		<resources>
			<resource ref="Smsto_Sms::Smslog_view"/>
		</resources>
	</route>
	<route method="PUT" url="/V1/smsto-sms/smslog/:logId">
		<service class="Smsto\Sms\Api\SmslogRepositoryInterface" method="save"/>
		<resources>
			<resource ref="Smsto_Sms::Smslog_update"/>
		</resources>
	</route>
	<route method="DELETE" url="/V1/smsto-sms/smslog/:logId">
		<service class="Smsto\Sms\Api\SmslogRepositoryInterface" method="deleteById"/>
		<resources>
			<resource ref="Smsto_Sms::Smslog_delete"/>
		</resources>
	</route>
</routes>
